{
    "version": 2,
    "functions": {
        "api/**/*.ts": {
            "runtime": "@vercel/node@3.0.0"
        }
    },
    "routes": [
        {
            "src": "/api/v1/status",
            "dest": "/api/status"
        },
        {
            "src": "/api/v1/installer",
            "dest": "/api/installer"
        },
        {
            "src": "/api/v1/fastflags",
            "dest": "/api/fastFlags"
        }
    ],
    "buildCommand": "npm run build",
    "outputDirectory": "dist"
}
